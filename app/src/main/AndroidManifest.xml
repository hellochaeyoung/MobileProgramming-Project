<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kr.ac.hansung.ume">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login.LoginActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Calendar.Calendar.CalendarActivity" />
        <activity android:name=".Calendar.Schedule.ScheduleEditActivity" />
        <activity android:name=".Login.AssignActivity" />
        <activity android:name=".Board.BoardActivity" />
        <activity android:name=".Chatting.ChatMessage" />
        <activity android:name=".View.HomeActivity" />
        <activity android:name=".Board.NewBoard"/>
        <activity android:name=".Board.DetailActivity"/>


        <service android:name="org.eclipse.paho.android.service.MqttService" />
    </application>

</manifest>